{
  "name": "@bootsandcats/mcp-oauth2-admin",
  "version": "1.0.0",
  "description": "Model Context Protocol server for managing OAuth2 Authorization Server at oauth2.cat-herding.net",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "mcp-oauth2-admin": "dist/index.js"
  },
  "scripts": {
    "build": "tsc && chmod +x dist/index.js",
    "watch": "tsc --watch",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write 'src/**/*.ts'"
  },
  "keywords": [
    "mcp",
    "oauth2",
    "authorization-server",
    "admin",
    "model-context-protocol"
  ],
  "author": "Bootsandcats Team",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "axios": "^1.6.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
